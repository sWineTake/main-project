<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twocow.song.mvc.repository.TestRepository">
	<select id="getTestConnectionList" resultType="com.twocow.song.mvc.vo.TestParameter">
		SELECT COLUMN1,
			   COLUMN2
		  FROM TEST
	</select>
	<update id="updateTestConnectionList">
		UPDATE TEST SET COLUMN2 = 'COLUMN1-1'
		 WHERE COLUMN1 = 1
	</update>
</mapper>
